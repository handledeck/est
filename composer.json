{
    "name": "handledeck/est-tools",
    "type": "library",
    "description": ":package_description",
    "keywords": [
        "Handledeck",
        "EstTools"
    ],
    "homepage": "https://github.com/Handledeck/est-tools",
    "license": "MIT",
    "authors": [
        {
            "name": "aleh",
            "email": "aleh@gmail.com",
            "homepage": "http://aleh.com",
            "role": "Developer"
        }
    ],
    "require": {
        "illuminate/support": "~5.1",
        "php" : "~5.5|~7.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "Handledeck\\EstTools\\": "src/"
        }
    },

    "autoload-dev": {
        "psr-4": {
            "Handledeck\\EstTools\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
